    setShowHamBurger(isSmallScreen ? true : false);\n    }\n\n    window.addEventListener(\"resize\", handleResize);\n\n    // clean up event listener to prevent memory leaks\n    return (_) => {\n      window.removeEventListener(\"resize\", handleResize);\n    };\n  });\n\n  const onHamBurgerClick = () => {\n    setShowSideBar(true);\n    setShowHamBurger(false);\n  };\n\n  const onCancelClick = () => {\n    setShowSideBar(false);\n    setShowHamBurger(true);\n  };\n\n  const style={\n    position: \"fixed\",\n    top: \"20px\",\n    right: \"20px\",\n    zIndex: \"2000\"\n}\n\n  return (\n    <Fragment>\n      {isSmallScreen && showHamBurger && (\n        <Hamburger\n          onHamBurgerClick={onHamBurgerClick}\n          style={style}\n        />\n      )}\n      {showSideBar && <Sidebar onCancelClick={onCancelClick} />}\n    </Fragment>\n  );\n};\n\nexport default Navigation;\n","/Users/jayashree/Documents/GitHub/FrontEndChallenge-MusicWebsite/music-website/src/components/Banner/Banner.js",[],"/Users/jayashree/Documents/GitHub/FrontEndChallenge-MusicWebsite/music-website/src/components/Hamburger/Hamburger.js",[],"/Users/jayashree/Documents/GitHub/FrontEndChallenge-MusicWebsite/music-website/src/constants.js",[],"/Users/jayashree/Documents/GitHub/FrontEndChallenge-MusicWebsite/music-website/src/Store.js",[],"/Users/jayashree/Documents/GitHub/FrontEndChallenge-MusicWebsite/music-website/src/RootReducer/RootReducer.js",[],"/Users/jayashree/Documents/GitHub/FrontEndChallenge-MusicWebsite/music-website/src/redux/album/types.js",[],"/Users/jayashree/Documents/GitHub/FrontEndChallenge-MusicWebsite/music-website/src/redux/album/action.js",[],"/Users/jayashree/Documents/GitHub/FrontEndChallenge-MusicWebsite/music-website/src/redux/album/reducer.js",[],"/Users/jayashree/Documents/GitHub/FrontEndChallenge-MusicWebsite/music-website/src/components/Card/Card.js",["121","122","123"],"/Users/jayashree/Documents/GitHub/FrontEndChallenge-MusicWebsite/music-website/src/component